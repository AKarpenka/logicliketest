{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      {
        "src": "/assets/(.*)",
        "dest": "/assets/$1",
        "headers": {
          "Cache-Control": "public, max-age=31536000, immutable",
          "Content-Type": "application/javascript"
        }
      },
      {
        "src": "/(.*\\.js)",
        "headers": {
          "Content-Type": "application/javascript"
        }
      },
      {
        "src": "/(.*\\.css)",
        "headers": {
          "Content-Type": "text/css"
        }
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }